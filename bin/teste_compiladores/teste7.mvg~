PROGRAMA TESTE;
VAR
 A, B: INTEIRO;
INÍCIO
  ESCREVA('DIGITE UM NÚMERO PARA A');
  LEIA(A);
  ESCREVA('DIGITE O NÚMERO PARA B');
  LEIA(B);
  SE (A > B) ENTÃO 
    ESCREVA('A É MAIOR QUE B')
  SENÃO 
    SE (A< B) ENTÃO
      ESCREVA('B É MAIOR QUE A')
    SENÃO;
      ESCREVA('B É IGUAL A A');
FIM.
