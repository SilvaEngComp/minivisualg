 PROGRAMA SOMAPARES;
 VAR
  SUPERIOR, SOMA, NUM: INTEIRO;
 INÍCIO
  SOMA<-0 + 2;
  ESCREVA ('ENTRE COM O LIMITE SUPERIOR');
  LEIA (SUPERIOR);
  NUM <- 2;
 
  ENQUANTO (NUM > SUPERIOR) FAÇA
  INÍCIO
   SOMA<-SOMA+NUM;
   NUM<-NUM+2;
  FIM;
 
  ESCREVA('A SOMA DOS NÚMEROS PARES DE 2 ATÉ ', SUPERIOR,' É ', SOMA);
  LEIA;
 FIM.
